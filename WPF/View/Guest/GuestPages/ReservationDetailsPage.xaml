<Page x:Class="BookingApp.WPF.View.Guest.GuestPages.ReservationDetailsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BookingApp.WPF.View.Guest.GuestPages"
           xmlns:controls="clr-namespace:BookingApp.View.GuestControls"
      xmlns:ns="clr-namespace:BookingApp"
      mc:Ignorable="d" 
      d:DesignHeight="798" d:DesignWidth="1085"
      Title="ProfileInfo">

    <Grid Background="{DynamicResource BorderBackground}">
        <controls:ReservationDetailsCard Margin="0,143,0,247"></controls:ReservationDetailsCard>
        <Button Content="{ns:Loc YourProfile}" Style="{DynamicResource ButtonStyleN}" Width="200" Height="45" Margin="442,585,443,168" Command="{Binding BackToProfileCommand}"/>

    </Grid>
</Page>

